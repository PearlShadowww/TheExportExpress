# Environment Configuration Example
# Copy this file to .env and update with your actual values

# Application Environment
RUN_MODE=development

# Database Configuration
EXPORT_MASTER_DATABASE_MONGODB_URI=mongodb://localhost:27017
EXPORT_MASTER_DATABASE_DATABASE_NAME=export_master_pro_dev

# Redis Configuration
EXPORT_MASTER_REDIS_REDIS_URI=redis://localhost:6379

# Security Configuration (Generate secure keys for production)
EXPORT_MASTER_SECURITY_ENCRYPTION_KEY=your-32-byte-encryption-key-here
EXPORT_MASTER_SECURITY_SALT=your-salt-here

# Logging Configuration
EXPORT_MASTER_LOGGING_LEVEL=debug

# Backup Configuration
EXPORT_MASTER_BACKUP_BACKUP_DIRECTORY=./backups